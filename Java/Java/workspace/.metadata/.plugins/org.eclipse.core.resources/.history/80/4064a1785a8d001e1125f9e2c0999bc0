import java.util.Scanner;
public class media {

	public static void main(String[] args) {
	Scanner in = new Scanner (System.in);
		
		final int TAM = 5; 
		
		int a[], b[], i,c;
		
		a= new int [TAM];
		b= new int [TAM];
		
		for (i=0; i<TAM; i++) { 
			System.out.println("Digite o "+(i+1)+" valor:");
			a[i] = in.nextInt(); 
			b[i] = (a[i]+a[i+1])/10;
		}
		b[i] = (a[i]+a[i+1])/10;
		System.out.print("\nB[] = [");
		for (i=0; i>=TAM; i++) { 
			System.out.print(b[i]+ " ");
			
		}
		System.out.print("]");

	}

}
